{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "babel"
    ],
    "rules": {
        "semi": [
            0,
            "never"
        ],
        "comma-dangle": [
            "error",
            "never"
        ],
        "indent": [
            "warn",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "linebreak-style": [
            1,
            "unix"
        ],
        "quotes": 0,
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-extra-semi": 2,
        "no-multiple-empty-lines": [
            1,
            {
                "max": 2
            }
        ],
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "error",
        "constructor-super": "warn",
        "valid-typeof": "warn",
        "no-console": "off",
        "space-before-blocks": "warn",
        "no-trailing-spaces": "warn",
        "arrow-spacing": "warn",
        "comma-spacing": [
            "warn",
            {
                "before": false,
                "after": true
            }
        ],
        "keyword-spacing": [
            "warn",
            {
                "before": true
            }
        ],
        "no-multi-spaces": 1,
        "key-spacing": [
            "warn",
            {
                "beforeColon": false,
                "afterColon": true
            }
        ],
        "space-infix-ops": [
            "warn",
            {
                "int32Hint": false
            }
        ],
        "react/display-name": 0,
        "react/prop-types": "warn",
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error",
        "react/no-unescaped-entities": "off"
    }
}